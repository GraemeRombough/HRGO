<Page loaded="pageLoaded" actionBarHidden="true"
    xmlns:CustomControls="custom-controls">
  <ActionBar title="HR GO" class="action-bar"></ActionBar>
  <GridLayout rows="*,auto">
  <StackLayout row="0" orientation="vertical" width="100%">
    <GridLayout columns="60,*,60" rows="auto" class="HeaderLabel">
      <label col="0" row="0" class="Header_Icon" text="&#xe991;"/>
      <label col="1" row="0" text="WHAT'S NEW" class="Header_Label"/>
      <Button col="2" row="0" text="&#xe967;" class="Back_Button" tap="goBack"/>
    </GridLayout>
    <ScrollView>
    <StackLayout orientation="vertical" id="NewsList">
        <!-- <Button class="Main_Nav_SubLine" text="Phoenix Compensation Agreement" id="1" tap="goToNotification"/>
        <Button class="Main_Nav_SubLine" text="New Surprise Announcement" id="2" tap="goToNotification"/> -->
    </StackLayout>
    </ScrollView>
  </StackLayout>
      
    
    <CustomControls:NavFooter row="1" />

    </GridLayout>

  <!-- Your UI components go here -->

</Page>