<Page loaded="pageLoaded" actionBarHidden="true">
  <ActionBar title="HR GO" class="action-bar"></ActionBar>
  <GridLayout rows="*,60">
  <StackLayout row="0" orientation="vertical" width="100%">
    <GridLayout columns="60,*" rows="auto" class="HeaderLabel">
      <label col="0" row="0" class="Header_Icon" text="&#xe93b;"/>
      <label col="1" row="0" text="MY STUDENT HIRING" class="Header_Label"/>
    </GridLayout>

    <StackLayout orientation="vertical">
      <Button class="Sub_Nav_Line" text="Student Hiring Initiative" tap="navToggle" value="Initiative"/>
        <Button class="Main_Nav_SubLine" id="11" text="Campaign for 19/20" tap="goToArticle" visibility="{{ Initiative ? 'visible' : 'collapsed' }}"/>
        <Button class="Main_Nav_SubLine" id="12" text="Commitment for 19/20" tap="goToArticle" visibility="{{ Initiative ? 'visible' : 'collapsed' }}"/>
        
      
      <Button class="Sub_Nav_Line" text="Student Hiring Process" tap="navToggle" value="Process"/>
        <Button class="Main_Nav_SubLine" id="13" text="National Student Hiring Team" tap="goToArticle" visibility="{{ Process ? 'visible' : 'collapsed' }}"/>
        <Button class="Main_Nav_SubLine" id="14" text="Student Security Screening" tap="goToArticle" visibility="{{ Process ? 'visible' : 'collapsed' }}"/>
        <Button class="Main_Nav_SubLine" id="15" text="Student Targeted Inventories" tap="goToArticle" visibility="{{ Process ? 'visible' : 'collapsed' }}"/>
        <Button class="Main_Nav_SubLine" id="16" text="E-Staffing and Student Re-Hires" tap="goToArticle" visibility="{{ Process ? 'visible' : 'collapsed' }}"/>
        

      <Button class="Sub_Nav_Line" text="Student FAQs" tap="navToggle" value="FAQ"/>
        <Button class="Main_Nav_SubLine" id="17" text="FAQs - FSWEP" tap="goToArticle" visibility="{{ FAQ ? 'visible' : 'collapsed' }}"/>
        <Button class="Main_Nav_SubLine" id="18" text="FAQs - Hiring a Former Student" tap="goToArticle" visibility="{{ FAQ ? 'visible' : 'collapsed' }}"/>
      

    
    </StackLayout>
  </StackLayout>
      
    
    <StackLayout row="1" class="FooterNav" orientation="horizontal">
        <Button class="Footer_NavIcon" text="&#xe902;" width="20%" tap="goToHome"/>
        <Button class="Footer_NavIcon" text="&#xe904;"  width="20%"/>
        <Button class="Footer_NavIcon" text="&#xe994;"  width="20%"/>
        <Button class="Footer_NavIcon" text="&#xe945;"  width="20%"/>
        <Button class="Footer_NavIcon" text="&#xe953;"  width="20%"/>
      </StackLayout>

    </GridLayout>

  <!-- Your UI components go here -->

</Page>